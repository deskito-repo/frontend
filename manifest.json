{
    "manifest_version": 3,
    "name": "deskito",
    "version": "0.0.2",
    "description": "wallpaper for browser",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_security_policy": {
      "script-src": "self",
      "object-src": "self"
    },
    "permissions": [
      "tabs",
      "storage",
      "history",
      "geolocation"
    ],
    "host_permissions": ["http://localhost:4124/*"]
}